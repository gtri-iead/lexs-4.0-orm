                        <nc:PersonSurName>Casillas</nc:PersonSurName>
                        <nc:PersonFullName>David Casillas</nc:PersonFullName>
                        <!-- Contact telephone number is required, other kinds of numbers optional -->
                        <nc:ContactTelephoneNumber>
