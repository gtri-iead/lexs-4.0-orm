                                <nc:FullTelephoneNumber>
                                    <nc:TelephoneNumberFullID>111-222-3333</nc:TelephoneNumberFullID>
                                </nc:FullTelephoneNumber>
                            </nc:ContactTelephoneNumber>
                        </lexs:DataOwnerContact>
                    </ulex:DataOwnerMetadata>
