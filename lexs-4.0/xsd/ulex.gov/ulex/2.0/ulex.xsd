<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document      : $Id: ulex.xsd 27499 2011-07-28 07:03:53Z ys34 $
    Namespace     : xmlns:ulex="http://ulex.gov/ulex/2.0"
    Description   : ULEX Messages Structure Format Schema
-->
<xsd:schema xmlns:ulex="http://ulex.gov/ulex/2.0" xmlns:ulexcodes="http://ulex.gov/codes/2.0"
            xmlns:ulexlib="http://ulex.gov/library/2.0"
            xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion"
            xmlns:wsa="http://www.w3.org/2005/08/addressing"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:wsnt="http://docs.oasis-open.org/wsn/b-2"
            targetNamespace="http://ulex.gov/ulex/2.0"
            elementFormDefault="qualified"
            attributeFormDefault="unqualified"
            version="2.0.PDSRDE.">
   <xsd:import namespace="http://ulex.gov/codes/2.0"
               schemaLocation="../../ulex-codes/2.0/ulex-codes.xsd"/>
   <xsd:import namespace="http://ulex.gov/library/2.0"
               schemaLocation="../../ulex-library/2.0/ulex-library.xsd"/>
   <xsd:import namespace="http://www.w3.org/2005/08/addressing"
               schemaLocation="../../../w3.org/ws-addr.xsd"/>
   <xsd:import namespace="urn:oasis:names:tc:SAML:2.0:assertion"
               schemaLocation="../../../oasis-open.org/security/saml/v2.0/saml-schema-assertion-2.0.xsd">
      <xsd:annotation>
         <xsd:documentation>An assertion schema for SAML V2.0 namespace.</xsd:documentation>
      </xsd:annotation>
   </xsd:import>
   <xsd:import namespace="http://docs.oasis-open.org/wsn/b-2"
               schemaLocation="../../../oasis-open.org/wsn/b-2/b-2.xsd">
      <xsd:annotation>
         <xsd:documentation>A Web Services Brokered Notification 1.3 schema.</xsd:documentation>
      </xsd:annotation>
   </xsd:import>
   <xsd:complexType name="AdvisoryType">
      <xsd:annotation>
         <xsd:documentation>A data type for an advisory that contains details about warnings. Advisory is not used if the result code is Success.  However, if there is a warning, there must be one or more advisory elements to provide additional details about the warning.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:AdvisoryCategoryCode"/>
         <xsd:element ref="ulex:AdvisoryText" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="AttachmentRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for an attachment request message. The responses to searches and get Data Item requests may include information about attachments, but in the interests of limiting the sizes of these results they do not include the attachments themselves.  This request allows a consumer to request the attachments.  Multiple attachments may be specified in a single request. All attachments are uniquely identified by AttachmentURI.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:RequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:AttachmentURI" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AttachmentResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for an attachment response message. This message may include multiple attachments.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:Attachment" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AttachmentSearchRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a message that defines a search for Data Items containing attachments matching specific criteria.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:SearchRequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:Attachment" minOccurs="0"/>
               <xsd:element ref="ulex:AttachmentSummaryConnectionQName" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="ulex:AttachmentHashValueAbstract" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AttachmentSummaryType">
      <xsd:annotation>
         <xsd:documentation>A data type for an attachment summary. Uniquely identifies the attachment by AttachmentURI and contains details about the attachment.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:AttachmentCategoryText" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentDescriptionText"/>
         <xsd:element ref="ulex:AttachmentFormatID" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentFormatStandardName" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentSizeValue" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentURI"/>
         <xsd:element ref="ulex:AttachmentViewableIndicator"/>
         <xsd:element ref="ulex:AttachmentSummaryExtensionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute ref="ulexlib:id"/>
   </xsd:complexType>
   <xsd:complexType name="AttachmentType">
      <xsd:annotation>
         <xsd:documentation>A data type for an attachment which is uniquely identified by an AttachmentURI and holds an attachment as defined in ulex:AttachmentContentAbstract.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:AttachmentURI" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentContentAbstract"/>
         <xsd:element ref="ulex:DataOwnerMetadata" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="AvailabilityResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for an availability response message. Message is returned by a service provider in response to a getAvailabilityRequest operation.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:ServiceAvailableIndicator"/>
               <xsd:element ref="ulex:AdditionalInformationText" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CapabilitiesResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a capabilities response message. Message is returned by a service provider in response to a getCapabilitiesRequest operation.  Some capabilities apply at the service provider level, and some only apply to structured or unstructured searches.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:Capabilities"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CapabilitiesType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of capabilities of a service provider.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DataItemCategory" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:PagingIndicator"/>
         <xsd:element ref="ulex:MaxHitsReturnedNumeric"/>
         <xsd:element ref="ulex:StructuredSearch" minOccurs="0"/>
         <xsd:element ref="ulex:TextSearch" minOccurs="0"/>
         <xsd:element ref="ulex:SubscribeNotify" minOccurs="0"/>
         <xsd:element ref="ulex:CapabilitiesExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="CommunityPedigreeURIType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Uniform Resource Identifier that uniquely identifies a Structured Payload schema that the Structured Payload is built on and links to. Pedigree number attribute indicates community pedigree order. The Digest is assigned pedigree 0, community receives pedigree 1, and etc.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="xsd:anyURI">
            <xsd:attribute ref="ulex:pedigreeNumber" use="required"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="DEMessageMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a Domain Exchange message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:MessageMetadataAbstractType">
            <xsd:sequence>
               <xsd:element ref="ulex:MessageOriginMetadata" minOccurs="0"/>
               <xsd:element ref="ulex:MessageDestinationIdentifierAbstract" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element ref="ulex:DEMessageMetadataExtensionAbstract" minOccurs="0"
                            maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DataItemCategoryType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Data Item category. DataItemCategory provides a set of information about the type of event or data type.  This may be used by service providers to indicate what types of events or data are incorporated in the service provider and/or its data sources, or in an individual Data Item.  Groups may work together to define an appropriate list of categories that can be used in searches among the groups to allow users to limit search results.  Structure could also be used by user interfaces for sorting purposes.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DataItemCategoryText"/>
         <xsd:element ref="ulex:DataItemCategoryDescriptionText" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DataItemGroupIdentifierType">
      <xsd:annotation>
         <xsd:documentation>A data type for a mechanism that groups Data Items that are related in some manner, or that may be related in some manner.  This mechanism provides an ID for the group as well as information about how and why they were grouped and who provided the grouping.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DataItemGroupID"/>
         <xsd:element ref="ulex:DataItemGroupCategoryText" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemGroupReasonText" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemGroupOriginatorID" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DataItemMetadataSearchRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a message that defines a search on Data Item metadata elements.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:SearchRequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:AttachmentSummaryConnectionQName" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="ulex:AttachmentRequiredIndicator" minOccurs="0"/>
               <xsd:element ref="ulex:DataItemAfterDate" minOccurs="0"/>
               <xsd:element ref="ulex:DataItemBeforeDate" minOccurs="0"/>
               <xsd:element ref="ulex:DataItemGroupAggregationIdentifier" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element ref="ulex:DataItemGroupCorrelationIdentifier" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element ref="ulex:DataItemKeywordsText" minOccurs="0"/>
               <xsd:element ref="ulex:DataItemReferenceID" minOccurs="0"/>
               <xsd:element ref="ulex:PackageMetadataExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DataItemPackageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Data Item package.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:PackageAbstractType">
            <xsd:sequence>
               <xsd:element ref="ulex:RenderingInstructions" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="ulex:NarrativeAbstract" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DataItemRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Data Item request message. Only a single Data Item may be requested in each message. Requested Data Item is uniquely identifier by DataItemID.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:RequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:DataItemID"/>
               <xsd:element ref="ulex:StructuredPayloadsRequestedAbstract"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DataItemResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Data Item response message. Message contains only a single Data Item, which may include information that was not in the response to a search operation.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:DataSubmitterMetadata" minOccurs="0"/>
               <xsd:element ref="ulex:DataItemPackage"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DataItemWithAttachmentsResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Data Item response message. Message contains only a single Data Item, which may include information that was not in the response to search operation. Message may also contain attachments associated with the Data Item.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:DataSubmitterMetadata" minOccurs="0"/>
               <xsd:element ref="ulex:DataItemPackage"/>
               <xsd:element ref="ulex:Attachment" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DataOwnerInformationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of detailed information about data owner capabilities, contained Data Item categories, etc.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DataOwnerMetadata"/>
         <xsd:element ref="ulex:StructuredSearchIndicator"/>
         <xsd:element ref="ulex:TextSearchIndicator"/>
         <xsd:element ref="ulex:DataItemCategory" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataOwnerInformationExtensionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DataOwnerMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies data owner.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DataOwnerIdentifierAbstract"/>
         <xsd:element ref="ulex:DataOwnerContactAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:DataOwnerMetadataExtensionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DataOwnersResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a message returned by a service provider in response to a getDataOwnersRequest. Message contains multiple elements that provide information about available data owners.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:DataOwnerInformation" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="DigestQueryStatementType">
      <xsd:annotation>
         <xsd:documentation>A data type for a query statement against a ULEX Digest. Structure serves as a wrapper for the Digest elements being searched, their values, and any modifiers (such as "fuzzy").  This element may not exist if the search is only being performed on Structured Payload elements.  This element includes a single Digest element with a single value; if multiple Digest elements are to be searched, or if multiple values are supplied for a single element, there must be multiple DigestQueryStatement blocks.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DigestQueryFieldAbstract" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:QueryMatchCode"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DomainActionMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a domain action message.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DEMessageMetadata"/>
         <xsd:element ref="ulex:DataSubmitterMetadata" minOccurs="0"/>
         <xsd:element ref="ulex:DomainAttribute" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DomainAttributeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a domain attribute. For Name/Value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:AttributeName" minOccurs="0"/>
         <xsd:element ref="ulex:AttributeValueText" minOccurs="0"/>
         <xsd:element ref="ulex:DomainName"/>
         <xsd:any processContents="lax" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DomainRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a domain request message.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DEMessageMetadata"/>
         <xsd:element ref="ulex:UserAssertionAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:UserAttentionToAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:DomainAttribute" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="DomainResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a domain response message.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DEMessageMetadata"/>
         <xsd:element ref="ulex:ResponseMetadata"/>
         <xsd:element ref="ulex:DomainAttribute" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="LinkagesType">
      <xsd:annotation>
         <xsd:documentation>A data type for a container that provides a single location for all connectors that link objects in different sections of a ULEX message, such as connectors that link a Digest object to a Structured Payload object, or a Structured Payload object to an object in a different Structured Payload, or an attachment summary to a Digest object, etc.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulexlib:SameAsConnection" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulexlib:AttachmentSummaryConnectionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MatchDetailsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of why a Data Item was matched in a search.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:MatchObjectDetails" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:SnippetText" minOccurs="0"/>
         <xsd:element ref="ulex:StructuredQueryMatchScorePercent" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MatchDigestElementReferenceType">
      <xsd:annotation>
         <xsd:documentation>A data type for a reference to a LEXS Digest element that was matched in a search, such as a person's last name.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulexlib:DigestObjectReference"/>
         <xsd:element ref="ulex:QueryMatchText" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MatchObjectDetailsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of search terms that were matched as part of an object hit.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:MatchDigestEntityReference" minOccurs="0"/>
         <xsd:element ref="ulex:MatchDigestElementReference" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:MatchStructuredPayloadEntityReference" minOccurs="0"/>
         <xsd:element ref="ulex:MatchStructuredPayloadElementReference" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="ulex:MatchObjectDescriptionText" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MatchStructuredPayloadElementReferenceType">
      <xsd:annotation>
         <xsd:documentation>A data type for a reference to a Structured Payload element that was matched in a search.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulexlib:StructuredPayloadObjectReference"/>
         <xsd:element ref="ulex:QueryMatchText" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="MessageMetadataAbstractType" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of metadata about a message.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:ULEXFrameworkVersionText"/>
         <xsd:element ref="ulex:ULEXImplementation"/>
         <xsd:element ref="ulex:MessageDateTime"/>
         <xsd:element ref="wsa:MessageID">
            <xsd:annotation>
               <xsd:documentation>An element (whose content is of type xs:anyURI) conveys the [message id] property.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="wsa:To" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>An element (whose content is of type xs:anyURI) provides the value for the [destination] property. If this element is NOT present then the value of the [destination] property is "http://www.w3.org/2005/08/addressing/anonymous".</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="wsa:From" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>An element (of type wsa:EndpointReferenceType) provides the value for the [source endpoint] property.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="wsa:ReplyTo" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>An element (of type wsa:EndpointReferenceType) provides the value for the [reply endpoint] property. If this element is NOT present then the value of the [address] property of the [reply endpoint] EPR is "http://www.w3.org/2005/08/addressing/anonymous".</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="wsa:FaultTo" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>An element (of type wsa:EndpointReferenceType) provides the value for the [fault endpoint] property.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="wsa:Action" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>An element (whose content is of type xs:anyURI) conveys the value of the [action] property.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="wsa:RelatesTo" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>An element information item contributes one abstract [relationship] property value, in the form of an (IRI, IRI) pair. The content of this element (of type xs:anyURI) conveys the [message id] of the related message.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="ulex:DataSensitivityAbstract" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="NumberOfStructuredPayloadTermsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a number of search terms used in a query for a given Structured Payload. This provides a count of search terms in any included Structured Payload elements so that service providers that do not understand one or more Structured Payloads will have a count of terms that were ignored for the purposes of calculating an accurate StructuredQueryMatchScorePercent. This element indicates the Structured Payload to which it applies and provides the number of search terms in that Structured Payload section.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:CommunityURI"/>
         <xsd:element ref="ulex:SearchTermsQuantity"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="OriginMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for metadata about a user, an organization, and a system where search query or retrieval request was originated.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:MessageOriginMetadata"/>
         <xsd:element ref="ulex:UserAssertionAbstract" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PDMessageMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a ULEX publish message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:MessageMetadataAbstractType">
            <xsd:sequence>
               <xsd:element ref="ulex:PDMessageMetadataExtensionAbstract" minOccurs="0"
                            maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="PackageAbstractType" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data type for a ULEX package. ULEX package is a base data structure for data exchange. Logical group of data normally viewed as a whole, such as a report.  Also referred to as a Data Item. This structure includes elements that are common to all packages. </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:PackageMetadata"/>
         <xsd:element ref="ulex:DigestAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:StructuredPayload" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:AttachmentSummary" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:Linkages" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PackageMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for metadata about a ULEX package.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DataItemID"/>
         <xsd:element ref="ulex:DataItemCompleteIndicator" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemContactAbstract" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataItemDate" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemGroupAggregationIdentifier" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataItemGroupCorrelationIdentifier" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataItemULEXImplementation" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemReferenceID"/>
         <xsd:element ref="ulex:DataItemPublishInstructionCode" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemStatusAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:DataOwnerMetadata" minOccurs="0"/>
         <xsd:element ref="ulex:DisseminationCriteriaAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:CaveatText" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemCategory" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataItemKeywordsText" minOccurs="0"/>
         <xsd:element ref="ulex:PackageMetadataExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="PercentSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a ratio, proper fraction, or percentage, with permissible decimal values ranging from 0 to 1inclusive.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:decimal">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="PublishMessageAcknowledgmentType">
      <xsd:annotation>
         <xsd:documentation>A data type for a publish message that allows the submitter to receive acknowledgment from the recipient that the message was received.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:PDMessageMetadata"/>
         <xsd:element ref="ulex:PublishMessageAcknowledgmentReference"/>
         <xsd:element ref="ulex:PublishMessageAcknowledgmentExtensionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PublishMessageContainerType">
      <xsd:annotation>
         <xsd:documentation>A data type for a ULEX publish message container. Structure wraps multiple ULEX publish messages and requires at least one message to publish.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:PublishMessage" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PublishMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a publish message.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:PDMessageMetadata"/>
         <xsd:element ref="ulex:DataSubmitterMetadata"/>
         <xsd:element ref="ulex:MessageItemAbstract" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RenderingInstructionsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of package rendering instructions for converting the XML data in the Data Item into a specified display format or instructions to display a pre-rendered version of the Data Item.  This may be provided as a stylesheet that can be used to display the package, or it may be a pre-rendered version (such as a JPEG or PDF image showing the contents of the package).  Rendering method can also be indicated as Narrative, which means there is no AttachmentURI and that the Narrative element is an alternate representation of the package.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:AttachmentURI" minOccurs="0"/>
         <xsd:element ref="ulex:AttachmentSizeValue" minOccurs="0"/>
         <xsd:element ref="ulex:RenderingMethodCode"/>
         <xsd:element ref="ulex:RenderingOutputFormatID"/>
         <xsd:element ref="ulex:RenderingOutputFormatStandardName"/>
         <xsd:element ref="ulex:RenderingParameter" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:RenderingDescriptionText" minOccurs="0"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RenderingParameterType">
      <xsd:annotation>
         <xsd:documentation>A data type for a rendering parameter for XSLT.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:RenderingParameterName"/>
         <xsd:element ref="ulex:RenderingParameterValueText"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a request message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:SRMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:UserAssertionAbstract" minOccurs="0"/>
               <xsd:element ref="ulex:UserAttentionToAbstract" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a response message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:SRMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:ResponseMetadata"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ResponseMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a response. This structure is included in all response results.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:ResultCode" minOccurs="0"/>
         <xsd:element ref="ulex:Advisory" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SNMessageMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a ULEX subscription/notification message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:MessageMetadataAbstractType">
            <xsd:sequence>
               <xsd:element ref="ulex:MessageOriginMetadata"/>
               <xsd:element ref="ulex:MessageDestinationIdentifierAbstract" maxOccurs="unbounded"/>
               <xsd:element ref="ulex:SNMessageMetadataExtensionAbstract" minOccurs="0"
                            maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SRMessageMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies search or retrieve message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:MessageMetadataAbstractType">
            <xsd:sequence>
               <xsd:element ref="ulex:MessageOriginMetadata"/>
               <xsd:element ref="ulex:MessageDestinationIdentifierAbstract" maxOccurs="unbounded"/>
               <xsd:element ref="ulex:SRMessageMetadataExtensionAbstract" minOccurs="0"
                            maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SRMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a generic LEXS SR message. Encapsulates SRMessageMetadata element describing the message.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:SRMessageMetadata"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SearchRequestMessageType" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data type for a search request message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:RequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:SearchRequestMetadata"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SearchRequestMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a search request.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:RequestedDataAbstract" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataOwnerIdentifierAbstract" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:MaxItemMatchesNumeric"/>
         <xsd:element ref="ulex:MatchBeginAfterText" minOccurs="0"/>
         <xsd:element ref="ulex:MatchEndBeforeText" minOccurs="0"/>
         <xsd:element ref="ulex:ServiceProviderSearchID" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemCategoryText" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:SortOrderAbstract" minOccurs="0"/>
         <xsd:element ref="ulex:TimeoutDuration" minOccurs="0"/>
         <xsd:element ref="ulex:NumberOfStructuredPayloadTerms" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:StructuredPayloadsRequestedAbstract"/>
         <xsd:element ref="ulex:SearchRequestMetadataExtensionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SearchResponseMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a search response message. Search response message contains "hits" from either a structured or text search.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:ResponseMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:SearchResponseMetadata"/>
               <xsd:element ref="ulex:SearchResultPackage" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SearchResponseMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a search response.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:MaxItemMatchesRequestedNumeric"/>
         <xsd:element ref="ulex:NumberItemMatchesText"/>
         <xsd:element ref="ulex:ServerLimitIndicator"/>
         <xsd:element ref="ulex:MatchBeginPointText" minOccurs="0"/>
         <xsd:element ref="ulex:MatchEndPointText" minOccurs="0"/>
         <xsd:element ref="ulex:PerformedStructuredQuery" minOccurs="0"/>
         <xsd:element ref="ulex:RequestedStructuredQuery" minOccurs="0"/>
         <xsd:element ref="ulex:ServiceProviderSearchID" minOccurs="0"/>
         <xsd:element ref="ulex:SearchResponseMetadataExtensionAbstract" minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SearchResultPackageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a search result package. </xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:PackageAbstractType">
            <xsd:sequence>
               <xsd:element ref="ulex:DataSubmitterMetadata" minOccurs="0"/>
               <xsd:element ref="ulex:MatchDetails" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="StructuredPayloadContentType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of data comprising the ULEX Structured Payload.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:any processContents="skip"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="StructuredPayloadMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for information that further qualifies a Structured Payload.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:CommunityURI"/>
         <xsd:element ref="ulex:CommunityDescriptionText" minOccurs="0"/>
         <xsd:element ref="ulex:CommunityVersionText"/>
         <xsd:element ref="ulex:CommunityPedigreeURI" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="StructuredPayloadQueryFieldType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Structured Payload query field. Structure provides a wrapper for a single Structured Payload object, where the object includes a single element with a single value.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:any processContents="skip"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="StructuredPayloadQueryStatementType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Structured Payload query statement. Provides a wrapper for Structured Payload elements being searched, their values, and any modifiers (such as "fuzzy").  This element includes a single Structured Payload element with a single value; if multiple Structured Payload elements are to be searched, or if multiple values are supplied for a single element, there must be multiple StructuredPayloadQueryStatement blocks.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:StructuredPayloadMetadata"/>
         <xsd:element ref="ulex:StructuredPayloadQueryField"/>
         <xsd:element ref="ulex:QueryMatchCode"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="StructuredPayloadType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Structured Payload and a description for it. Structured payload contains data geared towards a community, such as incidents or arrests, or a sub-community, such as bomb and arson incidents.  Builds on the contents of the Digest.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:StructuredPayloadMetadata"/>
         <xsd:element ref="ulex:StructuredPayloadContentAbstract"/>
      </xsd:sequence>
      <xsd:attribute ref="ulexlib:id"/>
   </xsd:complexType>
   <xsd:complexType name="StructuredPayloadsRequestedListType">
      <xsd:annotation>
         <xsd:documentation>A data type for a container for requested Structured Payloads.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:StructuredPayloadMetadata" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="StructuredQueryType">
      <xsd:annotation>
         <xsd:documentation>A data type for a ULEX structured query. Query is stated using the ULEX Digest and Structured Payload entities by supplying field values for selected elements.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:DigestQueryStatement" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:StructuredPayloadQueryStatement" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:StructuredQueryExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="StructuredSearchRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a structured search request message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:SearchRequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:StructuredQuery" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="StructuredSearchType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of structured search capabilities for a service provider.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:MultipleValueIndicator"/>
         <xsd:element ref="ulex:WildcardIndicator"/>
         <xsd:element ref="ulex:FuzzySearchIndicator"/>
         <xsd:element ref="ulex:DateRangeIndicator"/>
         <xsd:element ref="ulex:NumericRangeIndicator"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SubscribeNotifyType">
      <xsd:annotation>
         <xsd:documentation>A data type for a subscribe notify capability for a service provider.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:MatchSubscriptionIndicator"/>
         <xsd:element ref="ulex:IdenticalSearchSubscriptionIndicator"/>
         <xsd:element ref="ulex:SimilarSearchSubscriptionIndicator"/>
         <xsd:element ref="ulex:DataItemSearchSubscriptionIndicator"/>
         <xsd:element ref="ulex:DataItemRetrievalSubscriptionIndicator"/>
         <xsd:element ref="wsnt:NotificationProducerRP" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>Resource Properties for NotificationProducer. For details see section 4.1 NotificationProducer Resource Properties in Web Services Base Notification 1.3 (WS-BaseNotification)
OASIS Standard from 1 October 2006.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SubscriptionDataItemMetadataQueryType">
      <xsd:annotation>
         <xsd:documentation>A data type for a Data Item metadata query subscription.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:AttachmentSummaryConnectionQName" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="ulex:AttachmentRequiredIndicator" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemAfterDate" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemBeforeDate" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemGroupAggregationIdentifier" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataItemGroupCorrelationIdentifier" minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="ulex:DataItemKeywordsText" minOccurs="0"/>
         <xsd:element ref="ulex:DataItemReferenceID" minOccurs="0"/>
         <xsd:element ref="ulex:PackageMetadataExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SubscriptionStructuredQueryType">
      <xsd:annotation>
         <xsd:documentation>A data type for a structured query subscription.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:StructuredQuery" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SubscriptionTextQueryType">
      <xsd:annotation>
         <xsd:documentation>A data type for a text query subscription.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:QueryText"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="SystemMetadataType">
      <xsd:annotation>
         <xsd:documentation>A data type for metadata about an organization and a system where data was originated, was submitted from, or is being sent.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:SystemIdentifierAbstract"/>
         <xsd:element ref="ulex:SystemContactAbstract"/>
         <xsd:element ref="ulex:SystemMetadataExtensionAbstract" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="TextSearchRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A data type for a text search request message.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="ulex:SearchRequestMessageType">
            <xsd:sequence>
               <xsd:element ref="ulex:QueryText"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="TextSearchType">
      <xsd:annotation>
         <xsd:documentation>A data type for a text search capability for a service provider.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:ExactPhraseIndicator"/>
         <xsd:element ref="ulex:LogicalOperatorIndicator"/>
         <xsd:element ref="ulex:WildcardIndicator"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ULEXImplementationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a ULEX implementation identification using implementation version and name.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="ulex:ULEXImplementationVersionText"/>
         <xsd:element ref="ulex:ULEXImplementationName"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="UserAssertionSAMLType">
      <xsd:annotation>
         <xsd:documentation>A data type for a wrapper for a SAML 2.0 User Assertion.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element ref="saml:Assertion">
            <xsd:annotation>
               <xsd:documentation>A SAML assertion is a package of information that supplies zero or more statements made by a SAML authority; SAML authorities are sometimes referred to as asserting parties in discussions of assertion generation and exchange, and system entities that use received assertions are known as relying parties. SAML assertions are usually made about a subject, represented by the "Subject" element. However, the "Subject" element is optional, and other specifications and profiles may utilize the SAML assertion structure to make similar statements without specifying a subject, or possibly specifying the subject in an alternate way. Typically there are a number of service providers that can make use of assertions about a subject in order to control access and provide customized service, and accordingly they become the relying parties of an asserting party called an identity provider. </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="AdditionalInformationText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A set of additional information about the availability of a service provider. Element is populated by the service provider and can be used when the service is available, for example to indicate that a data owner is unavailable, or when the service is not available, for example to indicate that the service provider will be back up at a particular time.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="Advisory" type="ulex:AdvisoryType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An advisory that contains details about warnings. Advisory is not used if the result code is Success.  However, if there is a warning, there must be one or more advisory elements to provide additional details about the warning.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AdvisoryCategoryCode" type="ulexcodes:AdvisoryCategoryCodeSimpleType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A kind of warning advisory.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AdvisoryText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A text message that contains details about warnings.  Since this text may be provided to end-users, service providers should populate this text field with user friendly messages rather than technical jargon or stack traces.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="Attachment" type="ulex:AttachmentType"
                substitutionGroup="ulex:MessageItemAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>An attachment uniquely identified by an AttachmentURI and that holds an attachment as defined in ulex:AttachmentContentAbstract.  May contain data-related information such as a mug shot, fingerprint, sound file or other binaries or stylesheets used by Rendering Instructions.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentCategoryText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A kind of object that has been encoded.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentContentAbstract" abstract="true" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific attachment content elements, such as attachment binary and metadata.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentDescriptionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of an attachment.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:AttachmentExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>An attachment domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific attachment extension that is not addressed by the other ULEX attachment elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentFormatID" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An identifier for a file format or content type of an attachment.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentFormatStandardName" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A name of a standard or protocol used to classify attachment content.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentHashValueAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a value that represents the results of a hash function applied to an attachment binary.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentRequestMessage" type="ulex:AttachmentRequestMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A request for a ULEX attachment. Multiple attachments may be requested in a single message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentRequiredIndicator" type="xsd:boolean">
      <xsd:annotation>
         <xsd:documentation>True if the search should only return Data Items that contain one or more attachment; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentResponseMessage" type="ulex:AttachmentResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A response to a getAttachmentRequest. Message may include multiple attachments.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentSearchRequestMessage"
                type="ulex:AttachmentSearchRequestMessageType">
      <xsd:annotation>
         <xsd:documentation>A message that defines a search for Data Items containing attachments matching specific criteria.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentSizeValue" type="xsd:decimal" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A size of an attachment in kilobytes.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentSummary" type="ulex:AttachmentSummaryType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A summary of an attachment.  The summary exists at the package level and provides a way to link an attachment, which exists outside the package and that may not be included in the current message, and objects in the Digest and/or Structured Payloads.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentSummaryConnectionQName" type="xsd:QName">
      <xsd:annotation>
         <xsd:documentation>A name of the Digest attachment summary connection.  Allows a requestor to limit the kinds of attachments that should be returned in a query, for example to limit the attachments in a response to those that represent person facial images.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentSummaryDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:AttachmentSummaryExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>An attachment summary domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentSummaryExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific attachment summary extension that is not addressed by the other ULEX attachment summary elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentURI" type="xsd:anyURI" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Uniform Resource Identifier (URI) for an attachment.  Note that if the URI is provided as a URL, it does not imply that the attachment is available over the Internet at that address.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttachmentViewableIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if an attachment can be displayed (such as a mug shot); false otherwise (such as a stylesheet used for rendering).</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttributeName" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A name of a domain attribute. For example, RegionInfo.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AttributeValueText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A domain attribute value.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AvailabilityResponseMessage" type="ulex:AvailabilityResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A response to a getAvailabilityRequest. Shows the status of the service provider.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="Capabilities" type="ulex:CapabilitiesType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A set of capabilities of a service provider.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CapabilitiesDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:CapabilitiesExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A capabilities domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CapabilitiesExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific capabilities extension that is not addressed by the other ULEX capabilities elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CapabilitiesResponseMessage" type="ulex:CapabilitiesResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A response to a getCapabilitiesRequest.  Some capabilities apply at the service provider level, and some only apply to structured or unstructured searches.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CaveatText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A warning or caution.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CommunityDescriptionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of the community that originated a particular Structured Payload.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CommunityPedigreeURI" type="ulex:CommunityPedigreeURIType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Uniform Resource Identifier that uniquely identifies a Structured Payload schema that the Structured Payload is built on and links to. Pedigree number attribute indicates community pedigree order. The Digest is assigned pedigree 0, community receives pedigree 1, and etc.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CommunityURI" type="xsd:anyURI" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Structured Payload Community Uniform Resource Identifier to uniquely identify the community that created and/or is responsible for the Structured Payload.  Used in combination with CommunityVersionText to identify the Structured Payload uniquely.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="CommunityVersionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Structured Payload Community version. Used in combination with CommunityURI to identify the Structured Payload uniquely.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DEMessageMetadata" type="ulex:DEMessageMetadataType"
                substitutionGroup="ulex:MessageMetadataAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a ULEX domain exchange message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DEMessageMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:DEMessageMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A DE message metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DEMessageMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific DE message metadata extension that is not addressed by the other ULEX DE message metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemAfterDate" type="xsd:date" nillable="true">
      <xsd:annotation>
         <xsd:documentation>A date after which the Data Item was produced/generated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemBeforeDate" type="xsd:date" nillable="true">
      <xsd:annotation>
         <xsd:documentation>A date before which the Data Item was produced/generated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemCategory" type="ulex:DataItemCategoryType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A set of information about the type of event or data type.  This may be used by service providers to indicate what types of events or data are incorporated in the service provider and/or its data sources, or in an individual Data Item.  Groups may work together to define an appropriate list of categories that can be used in searches among the groups to allow users to limit search results.  Element could also be used by user interfaces for sorting purposes.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemCategoryDescriptionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of the Data Item category. Could be used to better explain the meaning of the DataItemCategoryText element value.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemCategoryText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A kind of Data Item category such as type of the event or a data type.  This should be a user-friendly and reasonably intuitive string that can be used in a list or as a check-box label in a GUI. Used as a search parameter in SearchRequestMetadata.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemCompleteIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the package includes all available information; false otherwise (i.e. more information is available). If the indicator does not exist, it does not imply either complete or incomplete; merely that the data owner either did not supply the indicator or does not want to indicate completeness for this Data Item.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemContactAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific Data Item contact elements, such as contact name or phone number.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemDate" type="xsd:date" nillable="true">
      <xsd:annotation>
         <xsd:documentation>A date the Data Item was produced or generated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemGroupAggregationIdentifier"
                type="ulex:DataItemGroupIdentifierType">
      <xsd:annotation>
         <xsd:documentation>An identification used to indicate that multiple Data Items are related, for example when multiple Data Items are grouped into a single case.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemGroupCategoryText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A kind of identifier being provided, such as a case number or a system generated identifier.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemGroupCorrelationIdentifier"
                type="ulex:DataItemGroupIdentifierType">
      <xsd:annotation>
         <xsd:documentation>An identification used to indicate that multiple Data Items MAY be related, for example when a tool or investigator has reason to believe that the Data Items are related.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemGroupID" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An identifier that indicates this Data Item is or may be related to other Data Items.  If more than one Data Item from the same data owner has the same value, then these Data Items are connected in some way.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemGroupOriginatorID" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An identifier of the originator responsible for grouping Data Items, such as system, domain, or person.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemGroupReasonText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A reason that indicates how or why a grouping was made, such as multiple incidents that were grouped by an investigator, or activities that were grouped based on automated analysis tools.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemID" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An identifier for the Data Item. DataItemID is unique for a given service provider.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemKeywordsText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A collection of keywords used for Data Item indexing.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemMetadataSearchRequestMessage"
                type="ulex:DataItemMetadataSearchRequestMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A message that defines a search on Data Item metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemPackage" type="ulex:DataItemPackageType"
                substitutionGroup="ulex:MessageItemAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Data Item package. </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemPublishInstructionCode"
                type="ulexcodes:DataItemPublishInstructionCodeSimpleType"
                nillable="true">
      <xsd:annotation>
         <xsd:documentation>A code that indicates whether to insert or delete a Data Item.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemReferenceID" type="xsd:string" nillable="true">
      <xsd:annotation>
         <xsd:documentation>An identifier that is meaningful to the owning system. This identifier is human readable, unique, and may be the same as the DataItemID.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemRequestMessage" type="ulex:DataItemRequestMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Data Item request message. Only a single Data Item may be requested in each message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemResponseMessage" type="ulex:DataItemResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Data Item response message returned in response to getDataItemRequest. Message always contains only a single Data Item, which may include information that was not in the doSearchResponse.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemRetrievalSubscriptionIndicator" type="xsd:boolean"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports Data Item retrieval subscription; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemSearchSubscriptionIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports Data Item search subscription; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemStatusAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific information that shows the status of the Data Item such as Valid/Invalid, etc.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemULEXImplementation" type="ulex:ULEXImplementationType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX implementation version and name used within the Data Item.  This version could differ from ULEXImplementation used in the MessageMetadata.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataItemWithAttachmentsResponseMessage"
                type="ulex:DataItemWithAttachmentsResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Data Item response message returned in response to getDataItemWithAttachmentsRequest. Message always contains only a single Data Item, which may include information that was not in the doSearchResponse. Message may also contain attachments associated with the Data Item.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerContactAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific data owner contact elements, such as contact name or phone number.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerIdentifierAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific data owner identifier elements that identify an organization and a system in an organization that owns data. This identifier is used in DataOwnerMetadata that is part of PackageMetadata, and in DataOwnerInformation that is part of DataOwnersResponseMessage.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerInformation" type="ulex:DataOwnerInformationType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A set of detailed information about data owner capabilities, contained Data Item categories, etc</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerInformationDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:DataOwnerInformationExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A data owner information domain attribute. For Name/Value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerInformationExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific information that is not addressed by the other ULEX data owner information elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerMetadata" type="ulex:DataOwnerMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about an organization that owns the data.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:DataOwnerMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A data owner metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnerMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific data owner metadata extension that is not addressed by the other ULEX data owner metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataOwnersResponseMessage" type="ulex:DataOwnersResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A response to a getDataOwnersRequest.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataSensitivityAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific information on the security classification level (e.g., SBU = Sensitive but Unclassified).  Note that this is different from dissemination criteria, which is included at the Data Item level.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DataSubmitterMetadata" type="ulex:SystemMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about an organization and a system where data was submitted from.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DateRangeIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if service provider supports search by date range; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DigestAbstract" abstract="true" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific standardized content elements determined by a program to be of broad interest.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DigestQueryFieldAbstract" abstract="true" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A data concept for a single Digest query field that wraps a single ULEX Entity, where the Entity includes a single element with a single value where allowed by schema.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DigestQueryStatement" type="ulex:DigestQueryStatementType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A query statement against a ULEX Digest. Structure serves as a wrapper for the Digest elements being searched, their values, and any modifiers (such as "fuzzy").  This element may not exist if the search is only being performed on Structured Payload elements.  This element includes a single Digest element with a single value; if multiple Digest elements are to be searched, or if multiple values are supplied for a single element, there must be multiple DigestQueryStatement blocks.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DisseminationCriteriaAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific information provided by the data owner to indicate who is allowed to see the information.  For publish it provides information to the service provider about what can be returned as the result of a search. For search or get results, it provides information to the user so they know the criteria for this information.  Note that this is different from the data sensitivity included at the message level.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DomainActionMessage" type="ulex:DomainActionMessageType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A domain action message used in the In-Only message exchange pattern, i.e. when no response is expected or required.  Includes a message identifier, ULEX version being used, and the date/time message was generated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DomainAttribute" type="ulex:DomainAttributeType">
      <xsd:annotation>
         <xsd:documentation>A generic domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DomainName" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A name of the domain to which this block of information applies, for example, OneDOJ.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DomainRequestMessage" type="ulex:DomainRequestMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A domain request message used in the In-Out message exchange pattern, i.e. when a response is expected or required.  Includes an identifier for this request, ULEX version being used by the requestor, and the date/time the request was generated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="DomainResponseMessage" type="ulex:DomainResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A response to a DomainRequestMessage.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ExactPhraseIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports exact phrases in text searches; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="FuzzySearchIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports some form of fuzzy search, such as soundex or metaphone; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="IdenticalSearchSubscriptionIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports search subscription; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="Linkages" type="ulex:LinkagesType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A container that provides a single location for all connectors that link objects in different sections of a ULEX message, such as connectors that link a Digest object to a Structured Payload object, or a Structured Payload object to an object in a different Structured Payload, or an attachment summary to a Digest object, etc.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="LogicalOperatorIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the logical operators are supported by the service provider in text searches; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchBeginAfterText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A value used by a service provider on a next/previous query to determine where the next set of hits should start.  It is populated with the value of MatchEndPointText from the response the user is following up. Value is of string type to give service provider as much flexibility as possible for point data format.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchBeginPointText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A value used by a service provider on a next/previous query to indicate the starting point for the set of data that is being returned.  Value is of string type to give service provider as much flexibility as possible for point data format.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchDetails" type="ulex:MatchDetailsType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of why a Data Item was matched in a search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchDigestElementReference" type="ulex:MatchDigestElementReferenceType">
      <xsd:annotation>
         <xsd:documentation>A LEXS Digest element that was matched in a search, such as a person's last name.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchDigestEntityReference" type="ulexlib:ValidatingObjectReferenceType">
      <xsd:annotation>
         <xsd:documentation>A LEXS Digest Entity that was matched in a search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchEndBeforeText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A value used by a service provider on a next/previous query to determine where the previous set of hits should end.  It is populated with the value of MatchBeginPointText from the response the user if following up.  Value is of string type to give service provider as much flexibility as possible for point data format.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchEndPointText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A value used by a service provider on a next/previous query to indicate the ending point for the set of data that is being returned. Value is of string type to give service provider as much flexibility as possible for point data format.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchObjectDescriptionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of how a high level object was matched in a search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchObjectDetails" type="ulex:MatchObjectDetailsType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A set of search terms that were matched as part of an object hit.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchStructuredPayloadElementReference"
                type="ulex:MatchStructuredPayloadElementReferenceType">
      <xsd:annotation>
         <xsd:documentation>A Structured Payload element that was matched in a search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchStructuredPayloadEntityReference"
                type="ulexlib:NonValidatingObjectReferenceType">
      <xsd:annotation>
         <xsd:documentation>A Structured Payload high level object that was matched in a search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MatchSubscriptionIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports match subscription; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MaxHitsReturnedNumeric" type="xsd:nonNegativeInteger" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A maximum number of hits that the service provider can return in a response.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MaxItemMatchesNumeric" type="xsd:nonNegativeInteger" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A maximum number of hits the user (or application acting on behalf of the user) wants back in a response.  Note that the service provider may override this value if the service provider's limit is less than the number requested.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MaxItemMatchesRequestedNumeric" type="xsd:nonNegativeInteger"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A maximum number of hits the user (or application acting on behalf of the user) requested.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MessageDateTime" type="xsd:dateTime" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A date and time when the message was created.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MessageDestinationIdentifierAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific elements that identify the organization and system to which an SR message is being sent.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MessageItemAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for an attachment or a Data Item package.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MessageMetadataAbstract" abstract="true" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A data concept for a message metadata.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MessageOriginMetadata" type="ulex:SystemMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about an organization and a system where message was originated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="MultipleValueIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider allows for multiple values in individual elements in a structured search; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="NarrativeAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for an unstructured content for the package.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="NumberItemMatchesText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A number of matches found, as opposed to returned, by the service provider.  Text field that can contain a number or an indication that there were more than a certain number of hits, such as 100+.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="NumberOfStructuredPayloadTerms"
                type="ulex:NumberOfStructuredPayloadTermsType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A number of search terms used in a query for a given Structured Payload. Structures provides a count of search terms in any included Structured Payload elements so that service providers that do not understand one or more Structured Payloads will have a count of terms that were ignored for the purposes of calculating an accurate MatchScore. This element indicates the Structured Payload to which it applies and provides the number of search terms in that Structured Payload section.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="NumericRangeIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports queries on ranges for numeric elements, such as a range on a boat length.  This also includes "open-ended" ranges, such as a search on a boat length less than a specified value.  Note that a service provider may support range searches on some numbers and not others; this Boolean does not indicate what elements, just that the service provider supports some level of numeric range searches; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PDMessageMetadata" type="ulex:PDMessageMetadataType"
                substitutionGroup="ulex:MessageMetadataAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a ULEX publish message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PDMessageMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:PDMessageMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A PD message metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PDMessageMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific PD message metadata extension that is not addressed by the other ULEX PD message metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PackageMetadata" type="ulex:PackageMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a ULEX package.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PackageMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:PackageMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A package metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PackageMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific metadata extension that is not addressed by the other ULEX package metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PagingIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports requests for additional hits from a search; false otherwise. </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PerformedStructuredQuery" type="ulex:StructuredQueryType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX Structured query, stated using the ULEX Digest and Structured Payload entities by supplying field values for selected elements. Structured query that was performed by the service provider and may differ from the query that was requested by the client.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PublishMessage" type="ulex:PublishMessageType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A publish message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PublishMessageAcknowledgment"
                type="ulex:PublishMessageAcknowledgmentType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A publish message that allows the submitter to receive acknowledgment from the recipient that the message was received.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PublishMessageAcknowledgmentDomainAttribute"
                type="ulex:DomainAttributeType"
                substitutionGroup="ulex:PublishMessageAcknowledgmentExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A publish message acknowledgment domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PublishMessageAcknowledgmentExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific publish message acknowledgment extension that is not addressed by the other ULEX publish message acknowledgment elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PublishMessageAcknowledgmentReference" type="wsa:EndpointReferenceType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A publish message that the acknowledgment is for.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="PublishMessageContainer" type="ulex:PublishMessageContainerType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A publish message container.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="QueryMatchCode" type="ulexcodes:QueryMatchCodeSimpleType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A kind of match the query requests for a query field.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="QueryMatchText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of how an element was matched in a search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="QueryText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX text query for querying unstructured data. Search utilizes wildcards and matching all terms, any terms, or exact phrase.  Logical operators AND as well as OR may be specified in the query string.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingDescriptionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A description of the rendered document.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingInstructions" type="ulex:RenderingInstructionsType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A set of package rendering instructions for converting the XML data in the Data Item into a specified display format or instructions to display a pre-rendered version of the Data Item.  This may be provided as a stylesheet that can be used to display the package, or it may be a pre-rendered version (such as a JPEG or PDF image showing the contents of the package).  Rendering method can also be indicated as Narrative, which means there is no AttachmentURI and that the Narrative element is an alternate representation of the package.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingMethodCode" type="ulexcodes:RenderingMethodCodeSimpleType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A code that identifies a rendering method.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingOutputFormatID" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An identifier for a file format or content type of a rendering output.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingOutputFormatStandardName" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A name of a standard or a protocol used to classify content of a rendering output.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingParameter" type="ulex:RenderingParameterType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A rendering parameter for XSLT.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingParameterName" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A name of an XSLT rendering parameter.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RenderingParameterValueText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An XSLT rendering parameter value.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RequestedDataAbstract" abstract="true" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific elements that identify the type of information being requested.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RequestedStructuredQuery" type="ulex:StructuredQueryType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX Structured query, stated using the ULEX Digest and Structured Payload entities by supplying field values for selected elements. Structured query that was requested by the client, which may differ from the query that was performed by the service provider.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ResponseMetadata" type="ulex:ResponseMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about the response message.  Indicates whether the request resulted in an error or warning or success (ResultCode), provides additional information about the result in cases of error or warning (Advisory). This element is included in all response results.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ResultCode" type="ulexcodes:ResultCodeSimpleType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A code that identifies a response status.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="RetrievalOriginMetadata" type="ulex:OriginMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a user, an organization, and a system where retrieval request was originated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SNMessageMetadata" type="ulex:SNMessageMetadataType"
                substitutionGroup="ulex:MessageMetadataAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a ULEX subscription/notification message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SNMessageMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:SNMessageMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>An SN message metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SNMessageMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific SN message metadata extension that is not addressed by the other ULEX SN message metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SRMessageMetadata" type="ulex:SRMessageMetadataType"
                substitutionGroup="ulex:MessageMetadataAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about an SR message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SRMessageMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:SRMessageMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A SR message metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SRMessageMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific SR message metadata extension that is not addressed by the other ULEX SR message metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchOriginMetadata" type="ulex:OriginMetadataType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a user, an organization, and a system where search query was originated.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchRequestMetadata" type="ulex:SearchRequestMetadataType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a search request.  This metadata is used in StructuredSearchRequestMessage in a doStructuredSearch request and in TextSearchRequestMessage in a doTextSearch request.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchRequestMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:SearchRequestMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A search request metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchRequestMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific SearchRequestMetadata extension that is not addressed by the other ULEX SearchRequestMetadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchResponseMessage" type="ulex:SearchResponseMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A search response message returned in response to structured or text search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchResponseMetadata" type="ulex:SearchResponseMetadataType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about a search response.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchResponseMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:SearchResponseMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A search response metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchResponseMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific SearchResponseMetadata extension that is not addressed by the other ULEX SearchResponseMetadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchResultPackage" type="ulex:SearchResultPackageType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A search result package. Contains information about the data owner that provided the data, a Digest, and possibly one or more Structured Payloads, narrative, snippet, rendering instructions, and attachment link elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SearchTermsQuantity" type="xsd:nonNegativeInteger" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A number of search terms in the Structured Payload.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ServerLimitIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the number of hits was restricted due to a service provider limit (the number returned was limited due to a server restriction); false otherwise. </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ServiceAvailableIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider is available; false otherwise (if there are problems, such as the database is down for maintenance). Note, that if there is no reply to the getAvailabilityRequest, it may indicate that the service provider is down, that there are network problems, that the requestor is not properly configured, etc.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ServiceProviderRequestMessage" type="ulex:SRMessageType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A service provider request message. Includes an identifier for this request, ULEX version being used by the requestor, and the date/time the request was generated.  The specific request that is being made is determined from the parent element name.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ServiceProviderSearchID" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>An identifier for a search that is meaningful to the service provider for returning additional hits.  This allows the service provider to utilize any state information it may retain so that it does not have to rerun the query again to get the next or previous set of results.  Service providers define this value; requesters merely copy the value back into the follow-up query.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SimilarSearchSubscriptionIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider supports search subscription; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SnippetText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A summary of information that represents the context of this Data Item in the responding system.  Conceptually similar to the results returned from a Google search.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SortOrderAbstract" abstract="true" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of program-specific information that indicates to the service provider what set of hits should be returned if there are more hits than can be returned in a single response.  </xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayload" type="ulex:StructuredPayloadType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX Structured Payload.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadContent" type="ulex:StructuredPayloadContentType"
                substitutionGroup="ulex:StructuredPayloadContentAbstract">
      <xsd:annotation>
         <xsd:documentation>A set of data comprising the ULEX Structured Payload.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadContentAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of data comprising the ULEX Structured Payload, which includes the default ULEX Structured Payload definition and that can be used by ULEX implementers to incorporate additional content.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadMetadata" type="ulex:StructuredPayloadMetadataType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>Metadata about Structured Payload.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadQueryField" type="ulex:StructuredPayloadQueryFieldType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Structured Payload query field.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadQueryStatement"
                type="ulex:StructuredPayloadQueryStatementType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A Structured Payload query statement.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadsRequestedAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a Structured Payload requested code or a list of Structured Payloads.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadsRequestedCode"
                type="ulexcodes:StructuredPayloadsRequestedCodeSimpleType"
                substitutionGroup="ulex:StructuredPayloadsRequestedAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A code that indicates whether responses should include all available Structured Payloads or no Structured Payloads.   "All" indicates that the requesting system would like to receive any Structured Payloads that the service provider can return.  "None" indicates that the requesting system does not want any Structured Payloads.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredPayloadsRequestedList"
                type="ulex:StructuredPayloadsRequestedListType"
                substitutionGroup="ulex:StructuredPayloadsRequestedAbstract"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A list of requested Structured Payloads. If this list element is provided instead of the code element, it means that the requesting system does not want to receive responses with any Structured Payloads except for the ones specified.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredQuery" type="ulex:StructuredQueryType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX Structured query, stated using the ULEX Digest and Structured Payload entities by supplying field values for selected elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredQueryDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:StructuredQueryExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A structured query domain attribute. For Name/Value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredQueryExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific structured query extension that is not addressed by the other ULEX structured query elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredQueryMatchScorePercent" type="ulex:PercentSimpleType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A structured query match score is a decimal between 0 and 1 representing a hit percentage.  Service providers will make best effort to answer queries the way they are asked.  Structured query match score represents the number of search elements that were matched for a structured search query.  A result that matches all requested fields is always a perfect match, represented by the value 1.  Match scores for structured queries where the service provider had to ignore one or more search terms will be provided using numbers less than 1 to represent the percentage.  Structured query match score is not applicable for text search results.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredSearch" type="ulex:StructuredSearchType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A service provider structured search capabilities.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredSearchIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the data owner includes structured data and can be searched using doStructuredSearchRequest; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="StructuredSearchRequestMessage"
                type="ulex:StructuredSearchRequestMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A structured search request message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SubscribeNotify" type="ulex:SubscribeNotifyType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A service provider subscribe notify capabilities.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SubscriptionDataItemMetadataQuery"
                type="ulex:SubscriptionDataItemMetadataQueryType"
                substitutionGroup="ulex:SubscriptionQueryAbstract">
      <xsd:annotation>
         <xsd:documentation>A Data Item metadata query subscription statement.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SubscriptionQueryAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a subscription query statement.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SubscriptionStructuredQuery" type="ulex:SubscriptionStructuredQueryType"
                substitutionGroup="ulex:SubscriptionQueryAbstract">
      <xsd:annotation>
         <xsd:documentation>A structured query subscription statement.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SubscriptionTextQuery" type="ulex:SubscriptionTextQueryType"
                substitutionGroup="ulex:SubscriptionQueryAbstract">
      <xsd:annotation>
         <xsd:documentation>A text query subscription statement.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SystemContactAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of contact information for the system owner.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SystemIdentifierAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a unique identifier of an organization and a system where data originated, was submitted from, or is being sent.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SystemMetadataDomainAttribute" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:SystemMetadataExtensionAbstract">
      <xsd:annotation>
         <xsd:documentation>A system metadata domain attribute. For name/value pair attributes use ulex:AttributeName and ulex:AttributeValueText elements. For hierarchical data xsd:any structure should be used.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="SystemMetadataExtensionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific system metadata extension that is not addressed by the other ULEX system metadata elements.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="TextSearch" type="ulex:TextSearchType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A service provider text search capabilities.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="TextSearchIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the data owner includes unstructured data and can be searched using doTextSearchRequest; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="TextSearchRequestMessage" type="ulex:TextSearchRequestMessageType"
                nillable="false">
      <xsd:annotation>
         <xsd:documentation>A text search request message.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="TimeoutDuration" type="xsd:duration" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A time in seconds within which the requesting system would like to receive a response. This element would primarily be used to convey a time period past which a response would not be useable by the requesting system. The time period specified excludes time for connection overhead to avoid the need for time synchronization between systems and thus should be slightly shorter than the period needed by the requesting system. The server, if capable of supporting such functionality, should cancel the search and return a timeout advisory response within the time limit specified. It is suggested that the search query should be timed out by the server prior to the time limit specified in order to allow time for canceling the query and returning a response before the period expires.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ULEXFrameworkVersionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX framework version used within the document, for example 2.0.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ULEXImplementation" type="ulex:ULEXImplementationType" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A ULEX implementation version and name used within the document.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ULEXImplementationName" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A name of the ULEX implementation used within the document, for example LEXS.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="ULEXImplementationVersionText" type="xsd:string" nillable="false">
      <xsd:annotation>
         <xsd:documentation>A version of the ULEX implementation used within the document, for example 4.0.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="UserAssertion" type="ulex:DomainAttributeType"
                substitutionGroup="ulex:UserAssertionAbstract">
      <xsd:annotation>
         <xsd:documentation>A user assertion that allows a domain to utilize name/value pair attributes or some other hierarchical user assertion when the provided user assertions are not applicable.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="UserAssertionAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific assertion indicating the user submitting a request.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="UserAssertionSAML" type="ulex:UserAssertionSAMLType"
                substitutionGroup="ulex:UserAssertionAbstract">
      <xsd:annotation>
         <xsd:documentation>A wrapper for a SAML 2.0 user assertion.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="UserAttentionToAbstract" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a program-specific assertion indicating the user to whose attention a response should be sent.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="WildcardIndicator" type="xsd:boolean" nillable="false">
      <xsd:annotation>
         <xsd:documentation>True if the service provider allows wildcard character(s) in structured searches; false otherwise.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:attribute name="pedigreeNumber" type="xsd:integer">
      <xsd:annotation>
         <xsd:documentation>A community pedigree order. The Digest is assigned pedigree 0, 1st community receives pedigree 1, and etc.</xsd:documentation>
      </xsd:annotation>
   </xsd:attribute>
</xsd:schema>