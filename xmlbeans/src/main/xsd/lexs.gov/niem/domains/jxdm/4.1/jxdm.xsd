        <xsd:sequence>
          <xsd:element ref="j:WarrantLevelText" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WitnessType">
