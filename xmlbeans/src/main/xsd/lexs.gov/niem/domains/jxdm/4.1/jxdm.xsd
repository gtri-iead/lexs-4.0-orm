    <xsd:annotation>
      <xsd:documentation>A data type for an association between a subject and a person.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
